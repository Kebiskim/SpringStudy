	// setter를 이용
	public void setDao(DAO dao) {
		this.dao = dao;
	}
	
	public DAO getDAO() {
		return dao;
	}
	

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public DAO getDao() {
		return dao;
	}

	// 실제 실행하고자 하는 메서드
	public void biz() {
		dao.prn();
	}
