package ex03_DI;

public class MyProcess {
	private String name = "홍길동";
	private int age = 13;
	public MyProcess() {

	}
	
	// 생성자 만들기 (source에서 generate using fields)
	public MyProcess(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	// 실행하고 싶은 메서드
	public void prn() {
		System.out.println("이름 : " + name);
		System.out.println("나이 : " + age);
	}
	
	
}
