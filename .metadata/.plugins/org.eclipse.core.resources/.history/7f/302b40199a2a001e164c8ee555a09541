package ex01_nonDI;

public class TestMain {
	public static void main(String[] args) {
		// 방법 1
		
		OracleDAO oracleDAO = new OracleDAO();
		MySQLDAO mySQLDAO = new MySQLDAO();
		
		// 실제 실행하고자 하는 메서드
		public void biz() {
			oracleDAO.prn();
			// mySQLDAO.prn();
		}
		
		// 방법 2
		// 클래스 자료형으로 활용
		private DAO dao;
		public Service() {}
		
		// 생성자를 이용해서
		
		Service service = new Service();
		service.biz();
	}
}
